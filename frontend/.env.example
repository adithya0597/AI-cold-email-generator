# JobPilot Frontend Environment Variables
# Copy this file to .env and fill in values

# Backend API URL (defaults to http://localhost:8000 if not set)
VITE_API_URL=http://localhost:8000

# Clerk Authentication (required for auth features in Plan 06)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_...

# Analytics (PostHog) -- leave empty to disable analytics
VITE_POSTHOG_KEY=
VITE_POSTHOG_HOST=https://us.i.posthog.com

# Sentry Error Tracking -- leave empty to disable
VITE_SENTRY_DSN=
SENTRY_AUTH_TOKEN=
